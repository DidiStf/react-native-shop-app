{
  "expo": {
    "name": "Shop App",
    "description": "A sample e-shop App.",
    "slug": "shop-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/shop-app-icon.png",
    "splash": {
      "image": "./assets/images/shop-app-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f7dfd4"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.didistf.shop-app",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/shop-app-icon.png",
        "backgroundColor": "#f7dfd4"
      },
      "package": "com.didistf.shop_app",
      "versionCode": 1,
      "useNextNotificationsApi": true
    }
  }
}